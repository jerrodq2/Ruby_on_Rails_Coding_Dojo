<h1>Listing Users</h1>
<table>
  <tr>
    <th>First Name</th><th>Last Name</th><th>Email</th><th>Actions</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user[:first_name] %></td>
    <td><%= user[:last_name] %></td>
    <td><%= user[:email] %></td>
    <td><%=link_to 'Show', user_path(user)%> | <%=link_to 'Edit', edit_user_path(user)%> | <%= link_to "Delete", user_path(user), method: :delete%></td>
  </tr>
  <% end %>
</table>
<a href="/users/new">New User</a>
